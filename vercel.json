{
  "version": 2,
  "builds": [
    { 
      "src": "src/main.ts", 
      "use": "@vercel/node", 
      "config": { "output": "dist" },
      "build": {"src": "src/main.ts", "use": {"command": "npm run build"} } }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
          "GET",
          "POST",
          "PUT",
          "PATCH",
          "DELETE"
      ]
    }
  ]
}
  